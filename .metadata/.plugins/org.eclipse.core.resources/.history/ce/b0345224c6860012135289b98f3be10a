package villa.math;

public final class VMath
{	
	public static double magnitude(double... mag)
	{
		double magnitude = 0;
		for(int i = 0; i < mag.length; i++)
		{
			magnitude += mag[i] * mag[i];
		}//rof
		magnitude = Math.sqrt(magnitude);
		
		return magnitude;
	}//END magnitude
}//END class VMath
